   		</action>
    </package>
    
    <package name="com.xyt.action" namespace="/xyt" extends="struts-default">
   	
   		<action name="XytCourseAction" class="XytCourseAction" >
   			<result name="totalcourse">/pages/xytpages/cources/totalcourse.jsp</result>
   			<result name="course">/pages/xytpages/cources/course.jsp</result>
   			
   		</action>
   		<action name="register" class="RegisterAction" >
   			<result name="showRegister">/pages/xytpages/register/showRegister.jsp</result>
    		<result name="error">/pages/xytpages/register/error.jsp</result>
   		</action>
   		<action name="pmessage" class="PmessageAction" >
   			<result name="showSendPmessage">/pages/xytpages/pmessage/showSendPmessage.jsp</result>
