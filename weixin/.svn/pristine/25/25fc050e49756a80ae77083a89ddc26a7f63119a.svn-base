   		<action name="XytCollegeAction" class="XytCollegeAction" >
   			<result name="mycampus">/pages/xytpages/mycampus/mycampus.jsp</result>
   			<result name="choosecampus">/pages/xytpages/mycampus/choosecampus.jsp</result>
   		</action>
