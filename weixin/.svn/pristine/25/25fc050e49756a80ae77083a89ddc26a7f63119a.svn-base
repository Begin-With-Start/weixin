    		<result name="error">/pages/xytpages/register/error.jsp</result>
   		</action>
   		<action name="XytGroupOrderAction" class="XytGroupOrderAction" >
   			<result name="groupInProceed">/pages/xytpages/group/totalgroup.jsp</result>
