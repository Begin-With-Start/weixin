@charset "utf-8";
/* CSS Document */

.creatgroup-main{padding:16px;}
.creatgroup-wrap{border-radius:3px; background-color:#fff; padding:16px;}
.input-bottom { border-bottom:1px solid #ebebeb;}
.input-group { position:relative; margin-top:10px;}
.p139-input { width:100%; box-sizing:border-box; color:#333; font-size:14px; border:1px #ddd solid; border-top-color: #BBB; padding-left:9px; line-height:41px; height:41px;  -webkit-appearance:none; -webkit-tap-highlight-color:transparent; outline:0; -webkit-border-radius: 0; border-radius:3px; }
.creatgroupPanle { background:-webkit-gradient(linear,0 0,0 100%,from(#f8f8f8),to(#e6e6e6));-webkit-box-shadow: 0 1px 1px 0 #c4c4c4; color:#333; font-size:16px; font-weight:600;}
.creatgroup-margin{ margin-top:10px;}
.creatgroup-main select {-webkit-appearance: none;width: 100%;height: 41px;-webkit-border-radius: 0;border: solid 1px #D5D5D5;border-top-color: #BDBDBD;border-width: 1px;color: #333333; font-size: 14px;padding-right: 16px; background: #ffffff; text-indent: 9px; margin-bottom: 10px;}
.creatgroup-main .creatgroup-select:after {content: "";top: 14px;right: 12px;position: absolute;display: inline-block;font-size: 0;border-top: 2px solid #666666;border-left: 2px solid #666666;width: 6px;height: 6px;background-color: transparent; -webkit-transform: rotate(225deg);}
.creatgroup-title { padding:10px 15px;}

