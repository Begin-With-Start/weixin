@charset "utf-8";
/* CSS Document */

.m-margin { margin:32px 0;}
.g-classDetail {width:100%; height:100%; font-family:'微软雅黑'; color:#a1a1a1;-webkit-overflow-scrolling:touch;-webkit-tap-highlight-color:rgba(0,0,0,0); background-color:#f1f1f1;}
.m-nav2 { width:100%; line-height:45px; color:#fff; background-color:#04BE02;z-index:10; height:45px;}
.m-nav2 h2 { text-align:center; font-size:16px; font-weight:normal; white-space:nowrap;}
.clearfix { display:block; zoom:1;}

/*底部菜单样式*/
.m-bottomPanel{ height:44px; width:100%; color:#fff; background-color:#04BE02; font-size:16px; text-align:center; line-height:44px; position:fixed; bottom:0;}
.m-bottomPanel-shoppingCart { width:49%; display:inline-block; float:left;}
.m-bottomPanel-line { border-right:1px #fff solid; width:1px; float:left; height:28px; margin-top:9px;}
.m-bottomPanel-pay { width:50%; display:inline-block;}

/*组团底部菜单*/
.js-order>input { border:none; background:transparent;}
.myteam,.creatgroup,.btleft,.btright{ width:49%; float:left; display:inline-block; font-size:16px; line-height:44px; color:#fff;}
.group-img-circle{border-radius:50%; -webkit-border-radius:50%;}

/*留言列表样式*/
.m-messages-area { padding:16px; border-bottom:1px solid #D9D9D9;}
.m-messages-title { font-size:17px; color:#393c3e; height:40px; font-weight:600; margin-bottom:20px;}
.m-messages-title img {  width:2.6em; height:2.6em; position:absolute;}

/*图片样式*/
.m-messages-area { padding:16px; border-bottom: 1px solid #D9D9D9;}
.m-messages-title { font-size: 17px; color: #393c3e; height: 40px; font-weight: 600; margin-bottom: 20px;}
.m-messages-title img { width: 2.6em; height: 2.6em;position: absolute;}
.m-messages-nickname { margin: 10px 0 0 53px;float: left; color:#aaa;}
.m-messages-time { color:#aaa; font-weight:normal;}
.m-messages-summary {font-size: 18px; color: #333;margin-top: 16px;text-indent: 2em;}
.img-circle {border-radius:50%; -webkit-border-radius:50%;}

