@charset "utf-8";
/* CSS Document */

#clonediv { padding:0 10px;}
#clonediv.pro { margin-bottom:20px;}
.mycode { font-size:18px; font-weight:700; line-height:1.5; color:#333;}
.code { font-size:12px; line-height:1.4em; color:#888;}
.myscore { font-size:12px; color:#ccc; line-height:2em; }
.score{ text-decoration:none; font-size:14px; color:#ff0000;}
div.pic { width:50%; overflow:hidden;}
div.pic img { vertical-align:middle; border:0px; font-size:0px; width:148px; height:148px;}
div.info { margin-left:10px; width:46%;}