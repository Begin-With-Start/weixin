@charset "utf-8";
/* CSS Document */

.group-main { padding:16px;}
.group-warp { border-radius:3px; background-color:#fff; padding:16px;}
.group-coursenema{font-size:18px; color:#393c3e; font-weight:600; margin-bottom:20px; }
.group-orignalfee,.group-groupfee { font-size:16px; color:#aaa; height:30px; font-weight:normal; line-height:30px;}
.group-groupfee>span { color:#ff7070;}
.group-img { width:100%; margin:0 auto; text-align:center;}
.group-warp img { width:15em;  padding:20px 0; }
.group-TercherInfo { text-align:center; font-size:16px; color:#333; height:30px; line-height:30px; margin-bottom:20px;}
.js-order>input { border:none; background:transparent;}
.myteam,.creatgroup { width:49%; float:left; display:inline-block; font-size:16px; line-height:44px; color:#fff;}
.p-h { height:40px; line-height:40px; border-top:1px #eee solid;}
.memberpic img { width:2.6em; height:2.6em; margin-right:10px;}
.avatars .avatar { margin:5px;}
.item { display: inline-block; vertical-align:middle; padding-bottom:5px;}
.avatar { width:2.4em; height:2.4em; display:inline-block; position:relative;}
.img-circle {border-radius:50%; -webkit-border-radius:50%;}
.avatar img { width:100%; height:100%; border:0.15em solid #fff;}
.avatar-sl img { width:2.6em; height:2.6em; padding:0;}