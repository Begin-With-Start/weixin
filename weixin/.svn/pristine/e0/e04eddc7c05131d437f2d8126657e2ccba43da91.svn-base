@charset "utf-8";
/* CSS Document */

.m-home-main { }
.m-home-wrap { padding-left:16px; padding-right:16px; padding-bottom:16px;}
.m-home-main>.m-home-wrap{ margin:0 auto; overflow:hidden; height:auto; min-height:auto; background-color:#fff;}
.m-home-wrap:before{ content:""; display:table;}
.pane { position:relative;}
.pane-personal { background-color:#fff; margin-bottom:0;}
.pane { margin-left:-15px; margin-right:-15px; padding:15px;}
.pane-wrap { padding:15px 30px; text-align:center;}
.avatar { width:2.4em; height:2.4em; display:inline-block; position:relative;}
.avatar-lg { width:100%; height:200px; display:block; margin:auto; text-align:center;}
.avatar-lg img { height:7.5em; width:7.5em;}
.avatar label { font-weight:normal; margin:15px auto; text-overflow:ellipsis; overflow:hidden; white-space:nowrap; display:block; color:#333;}
.avatar .personal_name { margin:23px 0 0 0; line-height:1;}
.color-blue{ color:#3690f8;}
.icon-gi-male:before { content: "\f16a";}
.pane-wrap .buttons { margin: 30px auto; margin: auto; margin-bottom: 0;}
.row { margin-left: -15px;margin-right: -15px;}
.col-xs-6 { width: 45%; float:left; position:relative; min-height:1px; padding-left:5px; padding-right:5px;}
.icon-gi-male img { height:16px; width:16px;}